{
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "file"
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "node_modules": true
  },
  "explorerExclude.backup": null,
  "cSpell.enableFiletypes": ["mdx"],
  "cSpell.words": ["clsx", "Infima"]
}
